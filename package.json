{
	"name": "git-go",
	"displayName": "Git Go",
	"description": "Manage git projects with a simple UI.",
	"publisher": "Carles Rojas",
	"license": "SEE LICENSE IN LICENSE file",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:git-go.open",
		"onCommand:git-go.refresh"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "git-go.open",
				"title": "Git Go: Open"
			},
			{
				"command": "git-go.refresh",
				"title": "Git Go: Refresh"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"vscode-test": "^1.5.2"
	}
}
